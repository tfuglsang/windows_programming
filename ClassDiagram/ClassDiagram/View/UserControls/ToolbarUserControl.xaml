<UserControl x:Class="ClassDiagram.View.UserControls.ToolbarUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ClassDiagram.View.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="40" d:DesignWidth="800">
    
    <ToolBar>
        <Button>
            <Button.Background>Aqua</Button.Background>
        </Button >
        <Button>
            <Button.Background>Aquamarine</Button.Background>
        </Button>
        <Button Name="Undo" Command="{Binding UndoCommand}">
            <Button.Background>DarkCyan</Button.Background>
        </Button>
        <Button Name="Redo" Command="{Binding RedoCommand}">
            <Button.Background>Brown</Button.Background>
        </Button>
    </ToolBar>
</UserControl>
